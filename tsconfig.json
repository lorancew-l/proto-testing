{
  "compilerOptions": {
    "declaration": true,
    "removeComments": true,
    "incremental": true,
    "strict": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "strictBindCallApply": false,
    "noFallthroughCasesInSwitch": true,
    "target": "ES2022"
  }
}
